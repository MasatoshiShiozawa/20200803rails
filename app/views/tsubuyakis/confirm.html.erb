<div class="container">
  <header class="header clearfix">
    <p>マイつぶやき</p>
  </header>
<h3>以下の内容で、送信する。</h3>
<%= form_with(model: @tsubuyaki, url: tsubuyakis_path, local: true) do |form| %>
  <%= form.hidden_field :content %>
  <p>本文:<%= @tsubuyaki.content %></p>
  <%= form.submit  "つぶやく" %>
  <%= form.submit "再編集", name: 'back' %>
<% end %>
<footer class="footer clearfix">
  <p>© TSUBUYAKI Corp. All RIGHTS RESERVED.</p>
</footer>
</div>
